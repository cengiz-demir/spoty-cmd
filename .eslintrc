{
  "parser": "babel-eslint",
  "env": {
	"browser": true,
	"node": true,
	"es6": true
  },
  "rules": {
	"strict": 1,
	"comma-dangle": ["error", "always-multiline"],
	"semi": ["error", "never"],
	"global-require": "error",
	"no-console": ["error", { allow: ["warn", "error"] }]
  },
  "extends": [
	"eslint:recommended",
	"customink/node",
	"customink/react"
  ]
}
